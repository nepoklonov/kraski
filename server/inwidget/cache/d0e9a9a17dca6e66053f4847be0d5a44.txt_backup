u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0445 \u0430\u0432\u0442\u043e\u0440\u043e\u0432 \u043e \u0427\u0443\u0432\u0430\u0448\u0438\u0438 \u0438 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u043c \u043d\u0430\u0440\u043e\u0434\u0435 \u043d\u0430 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u043c \u0438 \u0440\u0443\u0441\u0441\u043a\u043e\u043c \u044f\u0437\u044b\u043a\u0430\u0445 \u0434\u0435\u0442\u0435\u0439 \u0438 \u043c\u043e\u043b\u043e\u0434\u0435\u0436\u0438 \u0432 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u0435 \u043e\u0442 3 \u0434\u043e 18 \u043b\u0435\u0442\n\n\u0414\u043b\u044f \u0443\u0447\u0430\u0441\u0442\u0438\u044f \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e: \n\u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0443, \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u0435\u0451 \u043d\u0430 \u0441\u0430\u0439\u0442, \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0432 \u0430\u043d\u043a\u0435\u0442\u0443\n\n\u0421 2019 \u0433\u043e\u0434\u0430 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043d\u043e\u0441\u0438\u0442 \u0438\u043c\u044f \u041a\u043e\u043d\u0441\u0442\u0430\u043d\u0442\u0438\u043d\u0430 \u0418\u0432\u0430\u043d\u043e\u0432\u0430\n\n\u041a\u043e\u043d\u0441\u0442\u0430\u043d\u0442\u0438\u043d \u0412\u0430\u0441\u0438\u043b\u044c\u0435\u0432\u0438\u0447 \u0418\u0432\u0430\u043d\u043e\u0432 - \u043f\u043e\u044d\u0442, \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0447\u0438\u043a, \u043a\u043b\u0430\u0441\u0441\u0438\u043a \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u044b, \u0430\u0432\u0442\u043e\u0440 \u043f\u043e\u044d\u043c\u044b \u201c\u041d\u0430\u0440\u0441\u043f\u0438\u201d. \u0412\u044b\u043f\u0443\u0441\u043a\u043d\u0438\u043a \u0438 \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a \u0421\u0438\u043c\u0431\u0438\u0440\u0441\u043a\u043e\u0439 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0443\u0447\u0438\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u0439 \u0448\u043a\u043e\u043b\u044b. \u0417\u0430 \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u043a\u0443\u044e \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u043d\u0430\u043f\u0438\u0441\u0430\u043b \u0441\u0442\u0438\u0445\u043e\u0442\u0432\u043e\u0440\u0435\u043d\u0438\u044f, \u0441\u0442\u0438\u0445\u043e\u0442\u0432\u043e\u0440\u043d\u044b\u0435 \u0441\u043a\u0430\u0437\u043a\u0438, \u0431\u0430\u043b\u043b\u0430\u0434\u044b, \u0442\u0440\u0430\u0433\u0435\u0434\u0438\u0438, \u043f\u043e\u044d\u043c\u044b, \u0441\u0435\u0440\u0438\u044e \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u043e\u0432 \u043f\u043e \u0441\u0435\u043c\u0435\u0439\u043d\u044b\u043c \u043f\u0440\u0435\u0434\u0430\u043d\u0438\u044f\u043c. \u0412 \u043b\u0443\u0447\u0448\u0435\u043c \u0442\u0432\u043e\u0440\u0435\u043d\u0438\u0438 \u043f\u043e\u044d\u0442\u0430 \u2013 \u043f\u043e\u044d\u043c\u0435 \u201c\u041d\u0430\u0440\u0441\u043f\u0438\u201d \u2013  \u043e\u0441\u043c\u044b\u0441\u043b\u0435\u043d\u044b \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u0435 \u0444\u0438\u043b\u043e\u0441\u043e\u0444\u0441\u043a\u0438\u0435 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u044b: \u0447\u0435\u043b\u043e\u0432\u0435\u0447\u0435\u0441\u043a\u0430\u044f \u043b\u0438\u0447\u043d\u043e\u0441\u0442\u044c \u0438 \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u043e, \u0434\u043e\u0431\u0440\u043e \u0438 \u0437\u043b\u043e, \u0441\u043e\u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u044c\u043d\u044b\u0445 \u0438 \u0434\u0443\u0445\u043e\u0432\u043d\u044b\u0445 \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u0435\u0439, \u043f\u0440\u043e\u0442\u0438\u0432\u043e\u0431\u043e\u0440\u0441\u0442\u0432\u043e \u0436\u0438\u0437\u043d\u0438 \u0438 \u0441\u043c\u0435\u0440\u0442\u0438\u201d.(\u0410\u0440\u0442\u0435\u043c\u044c\u0435\u0432 \u042e. \u041c. \u0418\u0432\u0430\u043d\u043e\u0432\u2026 \u0421. 179) \u041a. \u0418\u0432\u0430\u043d\u043e\u0432 \u0432\u043e\u0448\u0435\u043b \u0432 \u0438\u0441\u0442\u043e\u0440\u0438\u044e \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u044b \u0438 \u043a\u0430\u043a \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0447\u0438\u043a. \u0411\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u0435\u043c\u0443 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u043d\u0430\u0440\u043e\u0434 \u0447\u0438\u0442\u0430\u043b \u043f\u0440\u043e\u0438\u0437\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u041c. \u041b\u0435\u0440\u043c\u043e\u043d\u0442\u043e\u0432\u0430, \u041d. \u041d\u0435\u043a\u0440\u0430\u0441\u043e\u0432\u0430, \u0410. \u041a\u043e\u043b\u044c\u0446\u043e\u0432\u0430, \u041d. \u041e\u0433\u0430\u0440\u0435\u0432\u0430, \u041a. \u0411\u0430\u043b\u044c\u043c\u043e\u043d\u0442\u0430. \u041a\u043b\u0430\u0441\u0441\u0438\u043a \u043f\u0440\u0438\u043d\u044f\u043b \u0430\u043a\u0442\u0438\u0432\u043d\u043e\u0435 \u0443\u0447\u0430\u0441\u0442\u0438\u0435 \u0432 \u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u043a\u0435 \u0441\u0431\u043e\u0440\u043d\u0438\u043a\u0430 \u201c\u041e\u0431\u0440\u0430\u0437\u0446\u044b \u043c\u043e\u0442\u0438\u0432\u043e\u0432 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0445 \u043d\u0430\u0440\u043e\u0434\u043d\u044b\u0445 \u043f\u0435\u0441\u0435\u043d \u0438 \u0442\u0435\u043a\u0441\u0442\u044b \u043a \u043d\u0438\u043c\u201d, \u043f\u0440\u0438 \u0435\u0433\u043e \u0443\u0447\u0430\u0441\u0442\u0438\u0438 \u0438\u0437\u0434\u0430\u043d \u201c\u0411\u0443\u043a\u0432\u0430\u0440\u044c \u0434\u043b\u044f \u0447\u0443\u0432\u0430\u0448 \u0441 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u0440\u0443\u0441\u0441\u043a\u043e\u0439 \u0430\u0437\u0431\u0443\u043a\u0438\u201d. \u041f\u043e\u044d\u0442 \u0443\u0441\u043f\u0435\u0448\u043d\u043e \u0440\u0430\u0431\u043e\u0442\u0430\u043b \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u0436\u0438\u0432\u043e\u043f\u0438\u0441\u0438: \u0440\u0438\u0441\u043e\u0432\u0430\u043b \u0434\u0435\u043a\u043e\u0440\u0430\u0446\u0438\u0438, \u0438\u043b\u043b\u044e\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0434\u043b\u044f \u0431\u0443\u043a\u0432\u0430\u0440\u044f, \u0443\u0432\u043b\u0435\u043a\u0430\u043b\u0441\u044f \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0435\u0439. \u0418\u043c\u0435\u043d\u0435\u043c \u041a. \u0418\u0432\u0430\u043d\u043e\u0432\u0430 \u0432 \u0433. \u0427\u0435\u0431\u043e\u043a\u0441\u0430\u0440\u044b \u043d\u0430\u0437\u0432\u0430\u043d\u0430 \u0443\u043b\u0438\u0446\u0430, \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u044b \u043f\u0430\u043c\u044f\u0442\u043d\u0438\u043a\u0438. \u0415\u0433\u043e \u0438\u043c\u0435\u043d\u0435\u043c \u0432 1966-1994 \u0433\u0433. \u043d\u0430\u0437\u044b\u0432\u0430\u043b\u0430\u0441\u044c \u0413\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f \u043f\u0440\u0435\u043c\u0438\u044f \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438. \u0412 \u0413\u043e\u0434 \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u044b \u0432 \u0420\u043e\u0441\u0441\u0438\u0438 \u0438 \u0432 \u0413\u043e\u0434 \u041a\u043e\u043d\u0441\u0442\u0430\u043d\u0442\u0438\u043d\u0430 \u0418\u0432\u0430\u043d\u043e\u0432\u0430 \u0432 \u0427\u0443\u0432\u0430\u0448\u0438\u0438 \u041d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u0430\u044f \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0430 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u0438\u043d\u0438\u0446\u0438\u0438\u0440\u043e\u0432\u0430\u043b\u0430 \u0438 \u0440\u0435\u0430\u043b\u0438\u0437\u043e\u0432\u0430\u043b\u0430 \u043c\u0435\u0436\u0434\u0443\u043d\u0430\u0440\u043e\u0434\u043d\u044b\u0439 \u043f\u0440\u043e\u0435\u043a\u0442 \u00ab\u0427\u0438\u0442\u0430\u0435\u043c \u00ab\u041d\u0430\u0440\u0441\u043f\u0438\u00bb. \u041f\u0440\u043e\u0435\u043a\u0442 \u043f\u0440\u0435\u0434\u043f\u043e\u043b\u0430\u0433\u0430\u0435\u0442 \u0447\u0442\u0435\u043d\u0438\u0435 \u043e\u0442\u0440\u044b\u0432\u043a\u043e\u0432 \u043f\u043e\u044d\u043c\u044b \u00ab\u041d\u0430\u0440\u0441\u043f\u0438\u00bb \u043d\u0430 \u0440\u0443\u0441\u0441\u043a\u043e\u043c \u044f\u0437\u044b\u043a\u0435 \u043d\u0430 \u0444\u043e\u043d\u0435 \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0445 \u0434\u043e\u0441\u0442\u043e\u043f\u0440\u0438\u043c\u0435\u0447\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0435\u0439 \u0438 \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u0435 \u0438\u0442\u043e\u0433\u043e\u0432\u043e\u0433\u043e \u0432\u0438\u0434\u0435\u043e \u0447\u0442\u0435\u043d\u0438\u044f \u043f\u043e\u044d\u043c\u044b.\n\n\u0421\u043e\u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u044b \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0430: \u041d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u0430\u044f \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a\u0430 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438, \u041e\u0431\u044a\u0435\u0434\u0438\u043d\u0435\u043d\u0438\u0435 \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a \u0433. \u0427\u0435\u0431\u043e\u043a\u0441\u0430\u0440\u044b.\n\n\u0421 \u041f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435\u043c \u043e \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u0441\u0430\u0439\u0442\u0435 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f\n\n@alena_arshinova\n#\u041a\u0440\u0430\u0441\u043a\u0438_\u0427\u0443\u0432\u0430\u0448\u0438\u0438 #kraski_konkurs","link":"https:\/\/www.instagram.com\/p\/B-bXskGjje8","type":"sidecar","fullsize":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/s1080x1080\/91423695_215666532873954_6940723105463067006_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=puuQ5L1E814AX8chMIK&oh=c787997f659860c7177aae7c14f65028&oe=5EACF361","large":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/sh0.08\/e35\/s640x640\/91423695_215666532873954_6940723105463067006_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=puuQ5L1E814AX8chMIK&oh=f429c5fe22d42ccf7711f3ba925686a7&oe=5EAE31D8","small":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/s320x320\/91423695_215666532873954_6940723105463067006_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=puuQ5L1E814AX8chMIK&oh=436ad55c67a6aad86f0dec42dbaa16ff&oe=5EAE3E81","likesCount":10,"commentsCount":0,"authorId":9227120312},{"id":"2277075491759127917","code":"B-ZzBhlDP1t","created":1585668573,"text":"[\u041a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0430\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430]\n\n\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0439 \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u043a\u0440\u0430\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u0438 \u0433\u0435\u043d\u0435\u0430\u043b\u043e\u0433\u0438\u0438 \u0438\u043c\u0435\u043d\u0438 \u041d\u0438\u043a\u043e\u043b\u0430\u044f \u041d\u0438\u043a\u043e\u043b\u044c\u0441\u043a\u043e\u0433\u043e\n\n\u041a \u0443\u0447\u0430\u0441\u0442\u0438\u044e \u0432 \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u044e\u0442\u0441\u044f \u0430\u0432\u0442\u043e\u0440\u0441\u043a\u0438\u0435 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u044f, \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u043d\u044b\u0435 \u0432\u043e\u043f\u0440\u043e\u0441\u0430\u043c \u043a\u0440\u0430\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u0438 \u0433\u0435\u043d\u0435\u0430\u043b\u043e\u0433\u0438\u0438 \u0440\u043e\u0434\u0430\n\n\u0414\u043b\u044f \u0443\u0447\u0430\u0441\u0442\u0438\u044f \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e: \n\u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0443, \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u0435\u0451 \u043d\u0430 \u0441\u0430\u0439\u0442, \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0432 \u0430\u043d\u043a\u0435\u0442\u0443 \n\u0421 2019 \u0433\u043e\u0434\u0430 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043d\u043e\u0441\u0438\u0442 \u0438\u043c\u044f \u041d\u0438\u043a\u043e\u043b\u0430\u044f \u041d\u0438\u043a\u043e\u043b\u044c\u0441\u043a\u043e\u0433\u043e\n\n\u041d\u0438\u043a\u043e\u043b\u0430\u0439 \u0412\u0430\u0441\u0438\u043b\u044c\u0435\u0432\u0438\u0447 \u041d\u0438\u043a\u043e\u043b\u044c\u0441\u043a\u0438\u0439 - \u0434\u043e\u043a\u0442\u043e\u0440 \u0438\u0441\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043d\u0430\u0443\u043a, \u044d\u0442\u043d\u043e\u0433\u0440\u0430\u0444, \u0444\u0438\u043b\u043e\u043b\u043e\u0433, \u043f\u0435\u0440\u0432\u044b\u0439 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440. \u0420\u043e\u0434\u0438\u043b\u0441\u044f \u0432 \u0434. \u042e\u0440\u043c\u0435\u043a\u0435\u0439\u043a\u0438\u043d\u043e \u041c\u043e\u0440\u0433\u0430\u0443\u0448\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438. \u0412\u044b\u043f\u0443\u0441\u043a\u043d\u0438\u043a \u041a\u0430\u0437\u0430\u043d\u0441\u043a\u043e\u0439 \u0434\u0443\u0445\u043e\u0432\u043d\u043e\u0439 \u0430\u043a\u0430\u0434\u0435\u043c\u0438\u0438. \u0412 1906 \u0433. \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043e\u0432\u0430\u043b \u0438\u0437\u0434\u0430\u043d\u0438\u0435 \u043f\u0435\u0440\u0432\u043e\u0439 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0433\u0430\u0437\u0435\u0442\u044b \u00ab\u0425\u044b\u043f\u0430\u0440\u00bb. \u0420\u0430\u0431\u043e\u0442\u0430\u043b \u0432 \u041a\u0430\u0437\u0430\u043d\u0441\u043a\u043e\u043c \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0435, \u0412\u043e\u0441\u0442\u043e\u0447\u043d\u043e\u0439 \u043a\u043e\u043d\u0441\u0435\u0440\u0432\u0430\u0442\u043e\u0440\u0438\u0438, \u0412\u043e\u0441\u0442\u043e\u0447\u043d\u043e\u043c \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0435. \u0412 1940-1950 \u0433\u0433. - \u0441\u0442\u0430\u0440\u0448\u0438\u0439 \u043d\u0430\u0443\u0447\u043d\u044b\u0439 \u0441\u043e\u0442\u0440\u0443\u0434\u043d\u0438\u043a \u041c\u0430\u0440\u0438\u0439\u0441\u043a\u043e\u0433\u043e \u043d\u0430\u0443\u0447\u043d\u043e-\u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u0433\u043e \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0430, \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440 \u041c\u0430\u0440\u0438\u0439\u0441\u043a\u043e\u0433\u043e \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0430. \u0410\u0432\u0442\u043e\u0440 \u043e\u043a\u043e\u043b\u043e 200 \u043d\u0430\u0443\u0447\u043d\u044b\u0445 \u0440\u0430\u0431\u043e\u0442. \u041f\u043e \u0441\u043e\u0437\u0434\u0430\u043d\u043d\u044b\u043c \u0438\u043c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430\u043c \u0441\u043e\u0431\u0440\u0430\u043d \u043e\u0433\u0440\u043e\u043c\u043d\u044b\u0439 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u043f\u043e \u0438\u0441\u0442\u043e\u0440\u0438\u0438, \u044d\u0442\u043d\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u0438 \u0444\u043e\u043b\u044c\u043a\u043b\u043e\u0440\u0443 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0433\u043e \u043d\u0430\u0440\u043e\u0434\u0430. \u041e\u0441\u043d\u043e\u0432\u043d\u044b\u0435 \u0438\u0437\u0434\u0430\u043d\u0438\u044f: \u042d\u0442\u043d\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0437\u0430\u043c\u0435\u0442\u043a\u0438 \u043e \u0447\u0443\u0432\u0430\u0448\u0430\u0445 \u041a\u043e\u0437\u044c\u043c\u043e\u0434\u0435\u043c\u044c\u044f\u043d\u0441\u043a\u043e\u0433\u043e \u0443\u0435\u0437\u0434\u0430 \u041a\u0430\u0437\u0430\u043d\u0441\u043a\u043e\u0439 \u0433\u0443\u0431\u0435\u0440\u043d\u0438\u0438\u00bb, \u00ab\u0425\u0440\u0438\u0441\u0442\u0438\u0430\u043d\u0441\u0442\u0432\u043e \u0441\u0440\u0435\u0434\u0438 \u0447\u0443\u0432\u0430\u0448\u0435\u0439 \u0421\u0440\u0435\u0434\u043d\u0435\u0433\u043e \u041f\u043e\u0432\u043e\u043b\u0436\u044c\u044f \u0432 XVI-XVIII \u0432\u0435\u043a\u0430\u0445\u00bb, \u00ab\u041a\u0440\u0430\u0442\u043a\u0438\u0439 \u043a\u0443\u0440\u0441 \u043f\u043e \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u0447\u0443\u0432\u0430\u0448\u00bb, \u00ab\u041a\u043e\u043d\u0441\u043f\u0435\u043a\u0442 \u043f\u043e \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u043d\u0430\u0440\u043e\u0434\u043d\u043e\u0439 \u043c\u0443\u0437\u044b\u043a\u0438 \u0443 \u043d\u0430\u0440\u043e\u0434\u043d\u043e\u0441\u0442\u0435\u0439 \u041f\u043e\u0432\u043e\u043b\u0436\u044c\u044f\u00bb, \u00ab\u041a\u0440\u0430\u0442\u043a\u0438\u0439 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e-\u0440\u0443\u0441\u0441\u043a\u0438\u0439 \u0441\u043b\u043e\u0432\u0430\u0440\u044c\u00bb \u0438 \u0434\u0440 \n\u0421\u043e\u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u044b \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0430: \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0439 \u043c\u0443\u0437\u0435\u0439, \u0413\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0438\u0441\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0430\u0440\u0445\u0438\u0432 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0440\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438\n\n\u0421 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435\u043c \u043e \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u043c \u0441\u0430\u0439\u0442\u0435 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f\n\n@alena_arshinova\n#\u041a\u0440\u0430\u0441\u043a\u0438_\u0427\u0443\u0432\u0430\u0448\u0438\u0438 #kraski_konkurs","link":"https:\/\/www.instagram.com\/p\/B-ZzBhlDP1t","type":"sidecar","fullsize":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/p1080x1080\/91473530_529124431345434_5578749861009825786_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=AiUClde0Q9MAX9HhZPL&oh=9b25d2ed5d8028cb84354e3542b5c159&oe=5EAD4894","large":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/sh0.08\/e35\/c0.5.1219.1219a\/s640x640\/91473530_529124431345434_5578749861009825786_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=AiUClde0Q9MAX9HhZPL&oh=e8513c9413c17d97ef0a12cc2120c81b&oe=5EAC7887","small":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/c0.5.1219.1219a\/s320x320\/91473530_529124431345434_5578749861009825786_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=AiUClde0Q9MAX9HhZPL&oh=2a40e3950a3ead49c1fc0eaf7f73bce4&oe=5EAE369F","likesCount":8,"commentsCount":0,"authorId":9227120312},{"id":"2276972303601276016","code":"B-Zbj8HjpRw","created":1585656272,"text":"[\u041a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0430\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430]\n\n\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043c\u0430\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0430 \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u043e\u0432 \u0443\u0447\u0440\u0435\u0436\u0434\u0435\u043d\u0438\u0439 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b \u0438 \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u0438\u043c\u0435\u043d\u0438 \u0413\u0435\u043d\u043d\u0430\u0434\u0438\u044f \u0412\u043e\u043b\u043a\u043e\u0432\u0430\n\n\u041a \u0443\u0447\u0430\u0441\u0442\u0438\u044e \u0432 \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u044e\u0442\u0441\u044f \u0430\u0432\u0442\u043e\u0440\u0441\u043a\u0438\u0435 \u043f\u0440\u043e\u0435\u043a\u0442\u044b \u0438 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0438, \u0441\u0446\u0435\u043d\u0430\u0440\u0438\u0438 \u043f\u0440\u0430\u0437\u0434\u043d\u0438\u043a\u043e\u0432, \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u0435\u0439, \u0442\u0435\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0443\u0440\u043e\u043a\u043e\u0432, \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u043d\u044b\u0445 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0435\n\n\u0414\u043b\u044f \u0443\u0447\u0430\u0441\u0442\u0438\u044f \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e: \n\u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0443, \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u0435\u0451 \u043d\u0430 \u0441\u0430\u0439\u0442, \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0432 \u0430\u043d\u043a\u0435\u0442\u0443. \n\u0421 2019 \u0433\u043e\u0434\u0430 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043d\u043e\u0441\u0438\u0442 \u0438\u043c\u044f \u0413\u0435\u043d\u043d\u0430\u0434\u0438\u044f \u0412\u043e\u043b\u043a\u043e\u0432\u0430.\n\n\u0413\u0435\u043d\u043d\u0430\u0434\u0438\u0439 \u041d\u0438\u043a\u0430\u043d\u0434\u0440\u043e\u0432\u0438\u0447 \u0412\u043e\u043b\u043a\u043e\u0432 - \u0432\u044b\u0434\u0430\u044e\u0449\u0438\u0439\u0441\u044f \u043f\u0435\u0434\u0430\u0433\u043e\u0433, \u0434\u043e\u043a\u0442\u043e\u0440 \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043d\u0430\u0443\u043a, \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440, \u043f\u0438\u0441\u0430\u0442\u0435\u043b\u044c, \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0447\u0438\u043a, \u043a\u0440\u0438\u0442\u0438\u043a, \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u044b\u0439 \u0443\u0447\u0438\u0442\u0435\u043b\u044c \u0448\u043a\u043e\u043b\u044b \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0410\u0421\u0421\u0420, \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u044b\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c \u043d\u0430\u0443\u043a\u0438 \u0420\u0424, \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u044b\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c \u043d\u0430\u0443\u043a\u0438 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u041a\u0430\u043b\u043c\u044b\u043a\u0438\u044f, \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u044b\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c \u043d\u0430\u0443\u043a\u0438 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u0422\u044b\u0432\u0430, \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u044b\u0439 \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u0421\u0430\u0445\u0430 (\u042f\u043a\u0443\u0442\u0438\u044f), \u043b\u0430\u0443\u0440\u0435\u0430\u0442 \u0413\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u043f\u0440\u0435\u043c\u0438\u0438 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438, \u0447\u043b\u0435\u043d \u0410\u043a\u0430\u0434\u0435\u043c\u0438\u0438 \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043d\u0430\u0443\u043a, \u043f\u043e\u0447\u0435\u0442\u043d\u044b\u0439 \u0434\u043e\u043a\u0442\u043e\u0440 \u042d\u0440\u0444\u0443\u0440\u0442\u0441\u043a\u043e\u0433\u043e \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0430, \u043f\u043e\u0447\u0435\u0442\u043d\u044b\u0439 \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0438\u043d \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438, \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u041a\u0430\u043b\u043c\u044b\u043a\u0438\u044f, \u042f\u043b\u044c\u0447\u0438\u043a\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430. \u0413. \u0412\u043e\u043b\u043a\u043e\u0432 \u2013 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0439 \u0432 \u0420\u043e\u0441\u0441\u0438\u0438 \u0438 \u0437\u0430 \u0440\u0443\u0431\u0435\u0436\u043e\u043c \u0443\u0447\u0435\u043d\u044b\u0439-\u043f\u0435\u0434\u0430\u0433\u043e\u0433, \u043e\u0441\u043d\u043e\u0432\u0430\u0442\u0435\u043b\u044c \u043d\u043e\u0432\u043e\u0433\u043e \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0432 \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u043d\u0430\u0443\u043a\u0435 \u2013 \u044d\u0442\u043d\u043e\u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u043a\u0438. \u041e\u043a\u043e\u043d\u0447\u0438\u043b \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442 \u0438\u043c. \u0418.\u042f. \u042f\u043a\u043e\u0432\u043b\u0435\u0432\u0430. \u0421 1952 \u043f\u043e 1972 \u0433\u0433. \u0440\u0430\u0431\u043e\u0442\u0430\u043b \u0432 \u044d\u0442\u043e\u043c \u0443\u0447\u0435\u0431\u043d\u043e\u043c \u0437\u0430\u0432\u0435\u0434\u0435\u043d\u0438\u0438. \u0411\u044b\u043b \u0437\u0430\u0432\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u0441\u0435\u043a\u0442\u043e\u0440\u043e\u043c \u043d\u0430\u0443\u0447\u043d\u043e-\u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u0433\u043e \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0430 \u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0445 \u0448\u043a\u043e\u043b \u041c\u0438\u043d\u0438\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0430 \u043d\u0430\u0440\u043e\u0434\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0441\u0432\u0435\u0449\u0435\u043d\u0438\u044f \u0420\u0421\u0424\u0421\u0420. \u041f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u043b \u0432 \u042d\u0440\u0444\u0443\u0440\u0442\u0441\u043a\u043e\u0439 \u0432\u044b\u0441\u0448\u0435\u0439 \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0448\u043a\u043e\u043b\u0435. \u0421 1993 \u0433. \u2013 \u0437\u0430\u0432\u0435\u0434\u0443\u044e\u0449\u0438\u0439 \u043b\u0430\u0431\u043e\u0440\u0430\u0442\u043e\u0440\u0438\u0435\u0439 \u044d\u0442\u043d\u043e\u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u043a\u0438 \u0413\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0430 \u0441\u0435\u043c\u044c\u0438 \u0438 \u0432\u043e\u0441\u043f\u0438\u0442\u0430\u043d\u0438\u044f. \u0421 1999 \u0433. \u2013 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440 \u041a\u0430\u043b\u043c\u044b\u0446\u043a\u043e\u0433\u043e \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0430. \u0412 2003 \u0433. \u043e\u0441\u043d\u043e\u0432\u0430\u043b \u043d\u0430\u0443\u0447\u043d\u043e-\u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u0438\u0439 \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442 \u044d\u0442\u043d\u043e\u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u043a\u0438 \u0432 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u043c \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u043c \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0435 \u0438\u043c. \u0418.\u042f. \u042f\u043a\u043e\u0432\u043b\u0435\u0432\u0430. \u0412 2005 \u0433. \u043e\u0442\u043a\u0440\u044b\u043b \u043d\u0430\u0443\u0447\u043d\u043e-\u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u0443\u044e \u043b\u0430\u0431\u043e\u0440\u0430\u0442\u043e\u0440\u0438\u044e \u044d\u0442\u043d\u043e\u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0438\u043d\u043d\u043e\u0432\u0430\u0446\u0438\u0439 \u041a\u0430\u043b\u043c\u044b\u0446\u043a\u043e\u0433\u043e \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0430. \u0410\u0432\u0442\u043e\u0440 \u0431\u043e\u043b\u0435\u0435 500 \u043d\u0430\u0443\u0447\u043d\u044b\u0445 \u0442\u0440\u0443\u0434\u043e\u0432.\n\n\u0421\u043e\u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u044b \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0430: \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b \u0424\u0430\u043a\u0443\u043b\u044c\u0442\u0435\u0442 \u0414\u0438\u041a\u041f\u0438\u041f \u0427\u0413\u041f\u0423 \u0438\u043c. \u0418.\u042f. \u042f\u043a\u043e\u0432\u043b\u0435\u0432\u0430.\n\nC \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435\u043c \u043e \u043a\u043e\u043d\u043a\u0443\u0440\u0435 \u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u0441\u0430\u0439\u0442\u0435 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f.\n\n@alena_arshinova\n#\u041a\u0440\u0430\u0441\u043a\u0438_\u0427\u0443\u0432\u0430\u0448\u0438\u0438 #kraski_konkurs","link":"https:\/\/www.instagram.com\/p\/B-Zbj8HjpRw","type":"sidecar","fullsize":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/p1080x1080\/91888012_1123244881348621_6980775565672519032_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=101&_nc_ohc=uDUqiTsAPlUAX-2ztUW&oh=f6e9838335afd5a3af594ef0c32f5c5a&oe=5EAF5131","large":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/sh0.08\/e35\/c0.5.1219.1219a\/s640x640\/91888012_1123244881348621_6980775565672519032_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=101&_nc_ohc=uDUqiTsAPlUAX-2ztUW&oh=d86893d282f46ee74d9578f3246c7522&oe=5EAFE7B0","small":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/c0.5.1219.1219a\/s320x320\/91888012_1123244881348621_6980775565672519032_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=101&_nc_ohc=uDUqiTsAPlUAX-2ztUW&oh=5bb628eba7102df6ce113092cb6b28e0&oe=5EAD8A91","likesCount":9,"commentsCount":0,"authorId":9227120312},{"id":"2276846092489286713","code":"B-Y-3U3D2Q5","created":1585641226,"text":"[\u041a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0430\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430]\n\n\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0434\u0435\u0442\u0441\u043a\u043e\u0433\u043e \u0442\u0430\u043d\u0446\u0435\u0432\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u0430 \u0438\u043c\u0435\u043d\u0438 \u041d\u0430\u0434\u0435\u0436\u0434\u044b \u041f\u0430\u0432\u043b\u043e\u0432\u043e\u0439\n\n\u041a \u0443\u0447\u0430\u0441\u0442\u0438\u044e \u0432 \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u044e\u0442\u0441\u044f: \u0434\u0435\u0442\u0441\u043a\u0438\u0435 \u0445\u043e\u0440\u0435\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0432\u044b\u0441\u0442\u0443\u043f\u043b\u0435\u043d\u0438\u044f\n\n\u0414\u043b\u044f \u0443\u0447\u0430\u0441\u0442\u0438\u044f \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e: \n\u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0443, \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u0435\u0451 \u043d\u0430 \u0441\u0430\u0439\u0442, \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0432 \u0430\u043d\u043a\u0435\u0442\u0443\n\n\u0421 2019 \u0433\u043e\u0434\u0430 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043d\u043e\u0441\u0438\u0442 \u0438\u043c\u044f \u041d\u0430\u0434\u0435\u0436\u0434\u044b \u041f\u0430\u0432\u043b\u043e\u0432\u043e\u0439\n\n\u041d\u0430\u0434\u0435\u0436\u0434\u0430 \u0412\u0430\u0441\u0438\u043b\u044c\u0435\u0432\u043d\u0430 \u041f\u0430\u0432\u043b\u043e\u0432\u0430 - \u0431\u0430\u043b\u0435\u0440\u0438\u043d\u0430, \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u0430\u044f \u0430\u0440\u0442\u0438\u0441\u0442\u043a\u0430 \u0420\u0421\u0424\u0421\u0420 , \u043d\u0430\u0440\u043e\u0434\u043d\u0430\u044f \u0430\u0440\u0442\u0438\u0441\u0442\u043a\u0430 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0410\u0421\u0421\u0420 , \u043d\u0430\u0440\u043e\u0434\u043d\u0430\u044f \u0430\u0440\u0442\u0438\u0441\u0442\u043a\u0430 \u0420\u0421\u0424\u0421\u0420, \u043d\u0430\u0440\u043e\u0434\u043d\u0430\u044f \u0430\u0440\u0442\u0438\u0441\u0442\u043a\u0430 \u0421\u0421\u0421\u0420. \u0420\u043e\u0434\u0438\u043b\u0430\u0441\u044c \u0432 \u0433. \u0427\u0435\u0431\u043e\u043a\u0441\u0430\u0440\u044b. \u041d\u0430\u0434\u0435\u0436\u0434\u0430 \u041f\u0430\u0432\u043b\u043e\u0432\u0430 \u2013 \u0432\u044b\u043f\u0443\u0441\u043a\u043d\u0438\u0446\u0430 \u041f\u0435\u0440\u043c\u0441\u043a\u043e\u0433\u043e \u0445\u043e\u0440\u0435\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0443\u0447\u0438\u043b\u0438\u0449\u0430, \u0413\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0430 \u0442\u0435\u0430\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u0430. \u0420\u0430\u0431\u043e\u0442\u0430\u043b\u0430 \u0432 \u0442\u0435\u0430\u0442\u0440\u0430\u0445 \u0433. \u041f\u0435\u0440\u043c\u044c. \u0412 1975-1995 \u0433\u0433. \u0431\u044b\u043b\u0430 \u0441\u043e\u043b\u0438\u0441\u0442\u043a\u043e\u0439 \u0431\u0430\u043b\u0435\u0442\u0430 \u0432 \u0411\u043e\u043b\u044c\u0448\u043e\u043c \u0442\u0435\u0430\u0442\u0440\u0435, \u0433\u0434\u0435 \u0435\u044e \u0438\u0441\u043f\u043e\u043b\u043d\u0435\u043d\u043e \u0431\u043e\u043b\u0435\u0435 20 \u0440\u043e\u043b\u0435\u0439. \u0412\u044b\u0441\u0442\u0443\u043f\u0430\u043b\u0430 \u0432 \u0410\u0432\u0441\u0442\u0440\u0438\u0438, \u042e\u0433\u043e\u0441\u043b\u0430\u0432\u0438\u0438, \u0418\u0442\u0430\u043b\u0438\u0438 \u0438 \u0434\u0440. \u041f\u043e \u0437\u0430\u0432\u0435\u0440\u0448\u0435\u043d\u0438\u0438 \u0431\u0430\u043b\u0435\u0442\u043d\u043e\u0439 \u043a\u0430\u0440\u044c\u0435\u0440\u044b \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0438\u043b\u0430 \u0440\u0430\u0431\u043e\u0442\u0443 \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0430-\u0440\u0435\u043f\u0435\u0442\u0438\u0442\u043e\u0440\u0430, \u0443\u0447\u0430\u0441\u0442\u0432\u043e\u0432\u0430\u043b\u0430 \u0432 \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u043a\u0438\u0445 \u0432\u0435\u0447\u0435\u0440\u0430\u0445 \u043c\u0430\u0441\u0442\u0435\u0440\u043e\u0432 \u043a\u043b\u0430\u0441\u0441\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0431\u0430\u043b\u0435\u0442\u0430. \u0421 1992 \u043f\u043e 1994 \u0433\u0433. \u2013 \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c \u0422\u0435\u0430\u0442\u0440\u0430 \u0431\u0430\u043b\u0435\u0442\u0430 \u041d\u0430\u0434\u0435\u0436\u0434\u044b \u041f\u0430\u0432\u043b\u043e\u0432\u043e\u0439, \u0430 \u0432 1995 \u0433. \u2013 \u0420\u0435\u043d\u0435\u0441\u0441\u0430\u043d\u0441-\u0431\u0430\u043b\u0435\u0442\u0430. \u0412 1999 \u0433. \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u043b\u0430 \u0443\u0447\u0430\u0441\u0442\u0438\u0435 \u0432 \u043e\u0442\u043a\u0440\u044b\u0442\u0438\u0438 \u0420\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u043e\u0433\u043e \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u043d\u043e\u0433\u043e \u0446\u0435\u043d\u0442\u0440\u0430 \u0432 \u0412\u0430\u0448\u0438\u043d\u0433\u0442\u043e\u043d\u0435, \u043b\u0430\u0443\u0440\u0435\u0430\u0442 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f \u00ab\u0417\u0432\u0435\u0437\u0434\u044b \u043c\u0438\u0440\u043e\u0432\u043e\u0433\u043e \u0431\u0430\u043b\u0435\u0442\u0430\u00bb. \u0412 1998-1999 \u0433\u0433. \u0412 2000 \u0433. \u0431\u044b\u043b\u0430 \u043f\u0435\u0434\u0430\u0433\u043e\u0433\u043e\u043c-\u0440\u0435\u043f\u0435\u0442\u0438\u0442\u043e\u0440\u043e\u043c \u0431\u0430\u043b\u0435\u0442\u043d\u043e\u0433\u043e \u0442\u0435\u0430\u0442\u0440\u0430 \u00ab\u0413\u0418\u0422\u0418\u0421\u00bb. \u0421 2009 \u0433. \u2013 \u0431\u0430\u043b\u0435\u0442\u043c\u0435\u0439\u0441\u0442\u0435\u0440-\u0440\u0435\u043f\u0435\u0442\u0438\u0442\u043e\u0440 \u0432 \u0413\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u043c \u0430\u043a\u0430\u0434\u0435\u043c\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u0442\u0435\u0430\u0442\u0440\u0435 \u043a\u043b\u0430\u0441\u0441\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0431\u0430\u043b\u0435\u0442\u0430. \u0421 2013 \u0433. \u2013 \u0431\u0430\u043b\u0435\u0442\u043c\u0435\u0439\u0441\u0442\u0435\u0440-\u0440\u0435\u043f\u0435\u0442\u0438\u0442\u043e\u0440 \u0431\u0430\u043b\u0435\u0442\u043d\u043e\u0439 \u0442\u0440\u0443\u043f\u043f\u044b \u0411\u043e\u043b\u044c\u0448\u043e\u0433\u043e \u0442\u0435\u0430\u0442\u0440\u0430. \u041f\u0440\u0435\u043f\u043e\u0434\u0430\u0435\u0442 \u0432 \u0420\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u043e\u0439 \u0430\u043a\u0430\u0434\u0435\u043c\u0438\u0438 \u0442\u0435\u0430\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u0430. \u0421\u043d\u044f\u043b\u0430\u0441\u044c \u0432 \u0433\u043b\u0430\u0432\u043d\u043e\u0439 \u0440\u043e\u043b\u0438 \u0432 \u0441\u043e\u0432\u0435\u0442\u0441\u043a\u043e-\u0430\u043c\u0435\u0440\u0438\u043a\u0430\u043d\u0441\u043a\u043e\u043c \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u043c \u0444\u0438\u043b\u044c\u043c\u0435 \u00ab\u0421\u0438\u043d\u044f\u044f \u043f\u0442\u0438\u0446\u0430\u00bb \u0438 \u0434\u0435\u0442\u0435\u043a\u0442\u0438\u0432\u043d\u043e\u0439 \u043a\u0438\u043d\u043e\u043b\u0435\u043d\u0442\u0435 \u00ab\u0421\u0438\u0446\u0438\u043b\u0438\u0430\u043d\u0441\u043a\u0430\u044f \u0437\u0430\u0449\u0438\u0442\u0430\u00bb. \u0422\u0430\u043d\u0435\u0446 \u041d. \u041f\u0430\u0432\u043b\u043e\u0432\u043e\u0439 \u0437\u0430\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u043d \u0432 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u043e\u043c \u0444\u0438\u043b\u044c\u043c\u0435 \u00ab\u0410\u0441\u0430\u0444 \u041c\u0435\u0441\u0441\u0435\u0440\u0435\u0440\u00bb , \u00ab\u0414\u043e\u0447\u0435\u0440\u0438 \u041e\u0442\u0447\u0438\u0437\u043d\u044b\u00bb, \u0432 \u0442\u0435\u043b\u0435\u0432\u0435\u0440\u0441\u0438\u0438 \u0431\u0430\u043b\u0435\u0442\u0430 \u00ab\u0414\u043e\u043d \u041a\u0438\u0445\u043e\u0442\u00bb\u0438 \u0442\u0435\u043b\u0435\u0444\u0438\u043b\u044c\u043c\u0435-\u0431\u0430\u043b\u0435\u0442\u0435 \u00ab\u041f\u043e\u044d\u043c\u044b\u00bb. \u0411\u0430\u043b\u0435\u0440\u0438\u043d\u0435 \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u044b \u0444\u0438\u043b\u044c\u043c\u044b \u00ab\u0422\u0430\u043d\u0446\u0443\u0435\u0442 \u041d\u0430\u0434\u044f \u041f\u0430\u0432\u043b\u043e\u0432\u0430\u00bb, \u00ab\u0414\u0436\u0443\u043b\u044c\u0435\u0442\u0442\u0430\u00bb , \u00ab\u0414\u0443\u044d\u0442 \u043c\u043e\u043b\u043e\u0434\u044b\u0445\u00bb , \u00ab\u041d\u0430\u0434\u0435\u0436\u0434\u0430 \u041f\u0430\u0432\u043b\u043e\u0432\u0430\u00bb. \u041e\u0441\u043d\u043e\u0432\u043d\u044b\u0435 \u043f\u0430\u0440\u0442\u0438\u0438: \u0421\u0432\u0430\u043d\u0438\u043b\u044c\u0434\u0430, \u0414\u0436\u0443\u043b\u044c\u0435\u0442\u0442\u0430, \u041a\u0438\u0442\u0440\u0438, \u0416\u0438\u0437\u0435\u043b\u044c, \u041c\u0430\u0448\u0430, \u0410\u0432\u0440\u043e\u0440\u0430, \u0424\u0440\u0438\u0433\u0438\u044f, \u042d\u043e\u043b\u0430 \u0438 \u0434\u0440. \u041d\u0430\u0433\u0440\u0430\u0436\u0434\u0435\u043d\u0430 \u043e\u0440\u0434\u0435\u043d\u043e\u043c \u00ab\u0417\u043d\u0430\u043a \u041f\u043e\u0447\u0435\u0442\u0430\u00bb\n\n\u0421\u043e\u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u044b \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0430: \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b \u0424\u0430\u043a\u0443\u043b\u044c\u0442\u0435\u0442 \u0414\u0438\u041a\u041f\u0438\u041f \u0427\u0413\u041f\u0423 \u0438\u043c. \u0418.\u042f. \u042f\u043a\u043e\u0432\u043b\u0435\u0432\u0430\n\n\u0421 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435\u043c \u043e \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u0441\u0430\u0439\u0442\u0435 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f\n\n@alena_arshinova\n#\u041a\u0440\u0430\u0441\u043a\u0438_\u0427\u0443\u0432\u0430\u0448\u0438\u0438 #kraski_konkurs","link":"https:\/\/www.instagram.com\/p\/B-Y-3U3D2Q5","type":"sidecar","fullsize":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/p1080x1080\/91398383_729390917595459_7894415899679777530_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=103&_nc_ohc=a58eSdSJc5UAX9WckKK&oh=f8b7b72c1dd1e49d63219bb6c3947822&oe=5EACC61A","large":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/sh0.08\/e35\/c0.5.1219.1219a\/s640x640\/91398383_729390917595459_7894415899679777530_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=103&_nc_ohc=a58eSdSJc5UAX9WckKK&oh=939d222f416420fb459f2802e9a4a4c8&oe=5EAD7E8D","small":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/c0.5.1219.1219a\/s320x320\/91398383_729390917595459_7894415899679777530_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=103&_nc_ohc=a58eSdSJc5UAX9WckKK&oh=9d91b2f35e0a5c716d34d15de6da3224&oe=5EAFF595","likesCount":9,"commentsCount":0,"authorId":9227120312},{"id":"2276331444576811700","code":"B-XJ2NpDXq0","created":1585579876,"text":"[\u041a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0430\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430]\n\n\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0434\u0435\u0442\u0441\u043a\u043e\u0433\u043e \u0432\u043e\u043a\u0430\u043b\u044c\u043d\u043e-\u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u0430 \u0438\u043c\u0435\u043d\u0438 \u041c\u0430\u043a\u0441\u0438\u043c\u0430 \u041c\u0438\u0445\u0430\u0439\u043b\u043e\u0432\u0430\n\n\u041a \u0443\u0447\u0430\u0441\u0442\u0438\u044e \u0432 \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u044e\u0442\u0441\u044f: \u0438\u0441\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u0435 \u043f\u0435\u0441\u043d\u0438 \u0434\u0435\u0442\u044c\u043c\u0438 \u0432 \u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u043e\u043c \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0435\u043d\u0438\u0438, \u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u043e\u0435 \u0438\u0441\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u0438\u0437\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u0434\u0435\u0442\u044c\u043c\u0438 (\u0434\u043e\u043f\u0443\u0441\u043a\u0430\u0435\u0442\u0441\u044f \u0443\u0447\u0430\u0441\u0442\u0438\u0435 \u0432\u0437\u0440\u043e\u0441\u043b\u044b\u0445)\n\n\u0414\u043b\u044f \u0443\u0447\u0430\u0441\u0442\u0438\u044f \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e: \n\u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0443, \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u0435\u0451 \u043d\u0430 \u0441\u0430\u0439\u0442, \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0432 \u0430\u043d\u043a\u0435\u0442\u0443\n\n\u0421 2019 \u0433\u043e\u0434\u0430 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043d\u043e\u0441\u0438\u0442 \u0438\u043c\u044f \u041c. \u0414. \u041c\u0438\u0445\u0430\u0439\u043b\u043e\u0432\u0430.\n\n\u041c\u0438\u0445\u0430\u0439\u043b\u043e\u0432 \u041c\u0430\u043a\u0441\u0438\u043c \u0414\u043e\u0440\u043c\u0438\u0434\u043e\u043d\u0442\u043e\u0432\u0438\u0447 - \u0432\u044b\u0434\u0430\u044e\u0449\u0438\u0439\u0441\u044f \u043e\u043f\u0435\u0440\u043d\u044b\u0439 \u043f\u0435\u0432\u0435\u0446, \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u044b\u0439 \u0430\u0440\u0442\u0438\u0441\u0442 \u0420\u0421\u0424\u0421\u0420, \u043d\u0430\u0440\u043e\u0434\u043d\u044b\u0439 \u0430\u0440\u0442\u0438\u0441\u0442 \u0421\u0421\u0421\u0420.  \u041d\u0430\u0447\u0430\u043b \u043f\u0435\u0442\u044c \u0432 \u0446\u0435\u0440\u043a\u043e\u0432\u043d\u043e\u043c \u0445\u043e\u0440\u0435 \u0421\u0440\u0435\u0442\u0435\u043d\u0441\u043a\u043e\u0433\u043e \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044f. \u0412 1914 \u0433. \u0437\u0430\u043a\u043e\u043d\u0447\u0438\u043b \u043f\u0430\u0441\u0442\u044b\u0440\u0441\u043a\u0438\u0435 \u043a\u0443\u0440\u0441\u044b \u0438 \u0431\u044b\u043b \u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d \u0434\u0438\u0430\u043a\u043e\u043d\u043e\u043c \u0432 \u0423\u0444\u0438\u043c\u0441\u043a\u0438\u0439 \u0441\u043e\u0431\u043e\u0440, \u0447\u0435\u0440\u0435\u0437 \u0442\u0440\u0438 \u0433\u043e\u0434\u0430 \u0431\u044b\u043b \u043f\u0435\u0440\u0435\u0432\u0435\u0434\u0435\u043d \u0432 \u0433. \u041e\u043c\u0441\u043a, \u0437\u0430\u0442\u0435\u043c \u2013 \u0432 \u0433. \u041a\u0430\u0437\u0430\u043d\u044c, \u0430 \u0432 1924 \u0433. \u2013 \u0432 \u041c\u043e\u0441\u043a\u0432\u0443, \u0433\u0434\u0435 \u0441\u043b\u0443\u0436\u0438\u043b  \u043f\u0440\u043e\u0442\u043e\u0434\u0438\u0430\u043a\u043e\u043d\u043e\u043c \u0432 \u0446\u0435\u0440\u043a\u0432\u0438 \u0412\u0430\u0441\u0438\u043b\u0438\u044f \u041a\u0435\u0441\u0430\u0440\u0438\u0439\u0441\u043a\u043e\u0433\u043e. \u0423\u0447\u0438\u043b\u0441\u044f \u0443 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0445 \u043e\u043f\u0435\u0440\u043d\u044b\u0445 \u043f\u0435\u0432\u0446\u043e\u0432 \u0424.\u0410. \u041e\u0448\u0443\u0441\u0442\u043e\u0432\u0438\u0447\u0430 \u0438 \u0412.\u0412. \u041e\u0441\u0438\u043f\u043e\u0432\u0430. \u0412 1930 \u0433. \u043e\u0441\u0442\u0430\u0432\u0438\u043b \u0441\u043b\u0443\u0436\u0435\u043d\u0438\u0435, \u043d\u0435 \u0441\u043d\u0438\u043c\u0430\u044f \u0441\u0430\u043d\u0430, \u0438 \u043f\u043e\u0441\u0442\u0443\u043f\u0438\u043b \u0432 \u043e\u043f\u0435\u0440\u043d\u0443\u044e \u0442\u0440\u0443\u043f\u043f\u0443 \u0420\u0430\u0434\u0438\u043e\u0446\u0435\u043d\u0442\u0440\u0430. \u0423\u0447\u0430\u0441\u0442\u0432\u043e\u0432\u0430\u043b \u0432 \u043e\u043f\u0435\u0440\u043d\u044b\u0445 \u0441\u043f\u0435\u043a\u0442\u0430\u043a\u043b\u044f\u0445. \u0412 \u043a\u043e\u043d\u0446\u0435 1932 \u0433. \u0431\u044b\u043b \u043f\u0440\u0438\u0433\u043b\u0430\u0448\u0435\u043d \u0432 \u0442\u0440\u0443\u043f\u043f\u0443 \u0411\u043e\u043b\u044c\u0448\u043e\u0433\u043e \u0442\u0435\u0430\u0442\u0440\u0430. 7 \u044f\u043d\u0432\u0430\u0440\u044f 1933 \u0433. \u0432\u043f\u0435\u0440\u0432\u044b\u0435 \u043f\u0435\u043b \u043f\u0430\u0440\u0442\u0438\u044e \u0412\u0430\u0440\u044f\u0436\u0441\u043a\u043e\u0433\u043e \u0433\u043e\u0441\u0442\u044f \u0432 \u043e\u043f\u0435\u0440\u0435 \u041d.\u0410. \u0420\u0438\u043c\u0441\u043a\u043e\u0433\u043e-\u041a\u043e\u0440\u0441\u0430\u043a\u043e\u0432\u0430 \u00ab\u0421\u0430\u0434\u043a\u043e\u00bb. \u0417\u0430\u0442\u0435\u043c \u0431\u044b\u043b\u0438 \u043f\u0430\u0440\u0442\u0438\u0438 \u041c\u0438\u0442\u044e\u0445\u0438 \u0438 \u041f\u0438\u043c\u0435\u043d\u0430 (\u00ab\u0411\u043e\u0440\u0438\u0441 \u0413\u043e\u0434\u0443\u043d\u043e\u0432\u00bb), \u0421\u0432\u0435\u0442\u043e\u0437\u0430\u0440\u0430 (\u00ab\u0420\u0443\u0441\u043b\u0430\u043d \u0438 \u041b\u044e\u0434\u043c\u0438\u043b\u0430\u00bb), \u041a\u043e\u043d\u0447\u0430\u043a\u0430 (\u00ab\u041a\u043d\u044f\u0437\u044c \u0418\u0433\u043e\u0440\u044c\u00bb), \u0421\u043e\u0431\u0430\u043a\u0438\u043d\u0430, \u0414\u0435\u0434\u0430 \u041c\u043e\u0440\u043e\u0437\u0430 (\u00ab\u0426\u0430\u0440\u0441\u043a\u0430\u044f \u043d\u0435\u0432\u0435\u0441\u0442\u0430\u00bb, \u00ab\u0421\u043d\u0435\u0433\u0443\u0440\u043e\u0447\u043a\u0430\u00bb), \u0427\u0443\u0431\u0430, \u0413\u0440\u0435\u043c\u0438\u043d\u0430 (\u00ab\u0427\u0435\u0440\u0435\u0432\u0438\u0447\u043a\u0438\u00bb, \u00ab\u0415\u0432\u0433\u0435\u043d\u0438\u0439 \u041e\u043d\u0435\u0433\u0438\u043d\u00bb). \u0421 17 \u043c\u0430\u0440\u0442\u0430 1939 \u0433. \u0438\u0441\u043f\u043e\u043b\u043d\u044f\u043b \u0440\u043e\u043b\u044c \u0418\u0432\u0430\u043d\u0430 \u0421\u0443\u0441\u0430\u043d\u0438\u043d\u0430 (\"\u0418\u0432\u0430\u043d \u0421\u0443\u0441\u0430\u043d\u0438\u043d (\u0416\u0438\u0437\u043d\u044c \u0437\u0430 \u0446\u0430\u0440\u044f)\"). \u0412 \u044d\u0442\u043e\u0439 \u043f\u0430\u0440\u0442\u0438\u0438, \u0441\u0442\u0430\u0432\u0448\u0435\u0439 \u0435\u0433\u043e \u0432\u044b\u0441\u0448\u0438\u043c \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u043a\u0438\u043c \u0434\u043e\u0441\u0442\u0438\u0436\u0435\u043d\u0438\u0435\u043c, \u0432\u044b\u0441\u0442\u0443\u043f\u0430\u043b \u0431\u043e\u043b\u0435\u0435 300 \u0440\u0430\u0437. \u0421 1951 \u0433. \u0433\u0430\u0441\u0442\u0440\u043e\u043b\u0438\u0440\u043e\u0432\u0430\u043b \u0437\u0430 \u0440\u0443\u0431\u0435\u0436\u043e\u043c. \u0421\u043d\u0438\u043c\u0430\u043b\u0441\u044f \u0432 \u043a\u0438\u043d\u043e\u0444\u0438\u043b\u044c\u043c\u0430\u0445 \u00ab\u0427\u0435\u0440\u0435\u0432\u0438\u0447\u043a\u0438\u00bb, \u00ab\u0418\u0432\u0430\u043d \u0413\u0440\u043e\u0437\u043d\u044b\u0439\u00bb, \u00ab\u0411\u043e\u0440\u0438\u0441 \u0413\u043e\u0434\u0443\u043d\u043e\u0432\u00bb . \u0412 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0435 \u0433\u043e\u0434\u044b \u0436\u0438\u0437\u043d\u0438 \u0431\u044b\u043b \u043d\u0435\u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u043e \u0437\u0430\u0431\u044b\u0442. \u041f\u0435\u0440\u0435\u0434 \u0441\u043c\u0435\u0440\u0442\u044c\u044e \u043f\u0440\u0438\u043d\u044f\u043b \u043c\u043e\u043d\u0430\u0448\u0435\u0441\u043a\u0438\u0439 \u043f\u043e\u0441\u0442\u0440\u0438\u0433. \u041f\u043e\u0445\u043e\u0440\u043e\u043d\u0435\u043d \u043d\u0430 \u041d\u043e\u0432\u043e\u0434\u0435\u0432\u0438\u0447\u044c\u0435\u043c \u043a\u043b\u0430\u0434\u0431\u0438\u0449\u0435. \u041d\u0430\u0433\u0440\u0430\u0436\u0434\u0435\u043d \u043e\u0440\u0434\u0435\u043d\u0430\u043c\u0438 \u041b\u0435\u043d\u0438\u043d\u0430 (1951), \u0422\u0440\u0443\u0434\u043e\u0432\u043e\u0433\u043e \u041a\u0440\u0430\u0441\u043d\u043e\u0433\u043e \u0417\u043d\u0430\u043c\u0435\u043d\u0438 (1937), \u043c\u0435\u0434\u0430\u043b\u044f\u043c\u0438. \u0412 \u0433. \u0427\u0435\u0431\u043e\u043a\u0441\u0430\u0440\u044b \u0441 1991 \u0433. \u043f\u0440\u043e\u0432\u043e\u0434\u0438\u0442\u0441\u044f \u0435\u0436\u0435\u0433\u043e\u0434\u043d\u044b\u0439 \u041c\u0435\u0436\u0434\u0443\u043d\u0430\u0440\u043e\u0434\u043d\u044b\u0439 \u043e\u043f\u0435\u0440\u043d\u044b\u0439 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044c \u0438\u043c. \u041c.\u0414. \u041c\u0438\u0445\u0430\u0439\u043b\u043e\u0432\u0430.\n\n\u0421\u043e\u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u044b \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0430: \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b \u0438 \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432 \u0424\u0430\u043a\u0443\u043b\u044c\u0442\u0435\u0442 \u0414\u0438\u041a\u041f\u0438\u041f \u0427\u0413\u041f\u0423 \u0438\u043c. \u0418.\u042f. \u042f\u043a\u043e\u0432\u043b\u0435\u0432\u0430\n\n\u0421 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435\u043c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u0430 \u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u0441\u0430\u0439\u0442\u0435 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f\n\n@alena_arshinova\n#\u041a\u0440\u0430\u0441\u043a\u0438_\u0427\u0443\u0432\u0430\u0448\u0438\u0438 #kraski_konkurs","link":"https:\/\/www.instagram.com\/p\/B-XJ2NpDXq0","type":"sidecar","fullsize":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/p1080x1080\/91115764_246298576500882_4457819081580704147_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=PmfaYU57yCwAX--cc5w&oh=9a88fba83ff4abb8b0e329a28936885a&oe=5EAF2515","large":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/sh0.08\/e35\/c0.5.1219.1219a\/s640x640\/91115764_246298576500882_4457819081580704147_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=PmfaYU57yCwAX--cc5w&oh=794795a2f4191170f2d2c4493a63d215&oe=5EAF4782","small":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/c0.5.1219.1219a\/s320x320\/91115764_246298576500882_4457819081580704147_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=106&_nc_ohc=PmfaYU57yCwAX--cc5w&oh=a1c4a481a02ea2ed88164010e82777ca&oe=5EAFA81A","likesCount":11,"commentsCount":0,"authorId":9227120312},{"id":"2276174639934666038","code":"B-WmMZ8DvU2","created":1585561183,"text":"[\u041a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0430\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430]\n\n\u041c\u043e\u043b\u043e\u0434\u0435\u0436\u043d\u044b\u0439 \u0444\u043e\u0442\u043e\u043a\u043e\u043d\u043a\u0443\u0440\u0441 \u0438\u043c\u0435\u043d\u0438 \u0431\u0440\u0430\u0442\u044c\u0435\u0432 \u041a\u043e\u0441\u0442\u0438\u043d\u044b\u0445\n\n\u041a \u0443\u0447\u0430\u0441\u0442\u0438\u044e \u0432 \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u044e\u0442\u0441\u044f \u0444\u043e\u0442\u043e\u0440\u0430\u0431\u043e\u0442\u044b \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0445 \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u043e\u0432 \u0438 \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u043e\u0432-\u043b\u044e\u0431\u0438\u0442\u0435\u043b\u0435\u0439, \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u043d\u044b\u0435 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0435\n\n\u0414\u043b\u044f \u0443\u0447\u0430\u0441\u0442\u0438\u044f \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e: \n\u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0443, \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u0435\u0451 \u043d\u0430 \u0441\u0430\u0439\u0442, \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0432 \u0430\u043d\u043a\u0435\u0442\u0443.\n\n\u0421 2019 \u0433\u043e\u0434\u0430 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043d\u043e\u0441\u0438\u0442 \u0438\u043c\u044f \u0431\u0440\u0430\u0442\u044c\u0435\u0432 \u041d\u0438\u043a\u043e\u043b\u0430\u044f, \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u0430 \u0438 \u0413\u0435\u043e\u0440\u0433\u0438\u044f \u041a\u043e\u0441\u0442\u0438\u043d\u044b\u0445 \n\u0411\u0440\u0430\u0442\u044c\u044f \u041a\u043e\u0441\u0442\u0438\u043d\u044b \u2014 \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u044b, \u0437\u0430\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u0432\u0448\u0438\u0435 \u043f\u0435\u0440\u0432\u044b\u0435 \u0448\u0430\u0433\u0438 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438, \u0440\u0430\u0431\u043e\u0442\u0430\u0432\u0448\u0438\u0435 \u0432 \u043f\u0435\u0440\u0432\u044b\u0445 \u0440\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0430\u043d\u0441\u043a\u0438\u0445 \u0438\u0437\u0434\u0430\u043d\u0438\u044f\u0445 \u0438 \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u0430\u0432\u0448\u0438\u0435 \u043e \u043f\u0435\u0440\u0432\u044b\u0445 \u0441\u0442\u0440\u043e\u0439\u043a\u0430\u0445, \u043a\u043e\u043b\u0445\u043e\u0437\u0430\u0445 \u0438 \u0430\u0440\u0442\u0435\u043b\u044f\u0445.\n\n\u0411\u0440\u0430\u0442\u044c\u0435\u0432, \u0437\u0430\u043d\u0438\u043c\u0430\u0432\u0448\u0438\u0445\u0441\u044f \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0435\u0439, \u0432 \u043c\u043d\u043e\u0433\u043e\u0434\u0435\u0442\u043d\u043e\u0439 \u0441\u0435\u043c\u044c\u0435 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u0430 \u0418\u0432\u0430\u043d\u043e\u0432\u0438\u0447\u0430 \u0438 \u0422\u0430\u0442\u044c\u044f\u043d\u044b \u0418\u043e\u043d\u043e\u0432\u043d\u044b \u041a\u043e\u0441\u0442\u0438\u043d\u044b\u0445 \u0431\u044b\u043b\u043e \u0442\u0440\u043e\u0435: \u0441\u0442\u0430\u0440\u0448\u0438\u0439 \u041d\u0438\u043a\u043e\u043b\u0430\u0439 \u0438 \u0431\u0440\u0430\u0442\u044c\u044f-\u0431\u043b\u0438\u0437\u043d\u0435\u0446\u044b \u0413\u0435\u043e\u0440\u0433\u0438\u0439 \u0438 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440. \u041f\u0435\u0440\u0432\u044b\u0439 \u0444\u043e\u0442\u043e\u0430\u043f\u043f\u0430\u0440\u0430\u0442 \u0441\u0442\u0430\u0440\u0448\u0435\u043c\u0443 \u0441\u044b\u043d\u0443 \u043a\u0443\u043f\u0438\u043b\u0430 \u043c\u0430\u043c\u0430. \u0418 \u041d\u0438\u043a\u043e\u043b\u0430\u0439 \u0442\u0443\u0442 \u0436\u0435 \u0441\u0442\u0430\u043b \u043f\u0440\u0438\u043e\u0431\u0449\u0430\u0442\u044c \u043a \u044d\u0442\u043e\u043c\u0443 \u0438 \u043c\u043b\u0430\u0434\u0448\u0438\u0445 \u0431\u0440\u0430\u0442\u044c\u0435\u0432, \u0431\u043e\u043b\u0435\u0435 \u0442\u043e\u0433\u043e \u2014 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043e\u0432\u0430\u043b \u0444\u043e\u0442\u043e\u043a\u0440\u0443\u0436\u043e\u043a. \u0421\u0440\u0430\u0437\u0443 \u043f\u043e\u0441\u043b\u0435 \u0448\u043a\u043e\u043b\u044b \u043e\u043d \u0441\u0442\u0430\u043b \u0443\u0447\u0435\u043d\u0438\u043a\u043e\u043c \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0430 \u0432 \u0444\u043e\u0442\u043e\u0430\u0442\u0435\u043b\u044c\u0435, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0441\u043e\u0442\u0440\u0443\u0434\u043d\u0438\u043a\u043e\u043c \u0433\u0430\u0437\u0435\u0442\u044b \u00ab\u041a\u0430\u043d\u0430\u0448\u00bb \u0438 \u0436\u0443\u0440\u043d\u0430\u043b\u0430 \u00ab\u0421\u0443\u043d\u0442\u0430\u043b\u00bb. \u0415\u0433\u043e \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u043f\u043e\u044f\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u0432 \u0446\u0435\u043d\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0439 \u00ab\u041a\u0440\u0435\u0441\u0442\u044c\u044f\u043d\u0441\u043a\u043e\u0439 \u0433\u0430\u0437\u0435\u0442\u0435\u00bb. \u041f\u043e\u0437\u0436\u0435 \u041d\u0438\u043a\u043e\u043b\u0430\u0439 \u041a\u043e\u0441\u0442\u0438\u043d \u0440\u0430\u0431\u043e\u0442\u0430\u043b \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u043e\u043c \u0432 \u043a\u0430\u0437\u0430\u043d\u0441\u043a\u043e\u043c \u0414\u043e\u043c\u0435 \u041a\u0440\u0430\u0441\u043d\u043e\u0439 \u0410\u0440\u043c\u0438\u0438, \u043d\u043e \u0435\u0433\u043e \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0430\u044e\u0442 \u043f\u0443\u0431\u043b\u0438\u043a\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u0432 \u0427\u0443\u0432\u0430\u0448\u0438\u0438. \u041c\u043b\u0430\u0434\u0448\u0438\u0435 \u0431\u0440\u0430\u0442\u044c\u044f \u0442\u043e\u0436\u0435 \u043d\u0430\u0447\u0438\u043d\u0430\u043b\u0438 \u0443\u0447\u0438\u0442\u044c\u0441\u044f \u0440\u0435\u043c\u0435\u0441\u043b\u0443 \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0430 \u0432 \u0442\u043e\u043c \u0436\u0435 \u0444\u043e\u0442\u043e\u0430\u0442\u0435\u043b\u044c\u0435. \u0417\u0430\u0442\u0435\u043c \u0413\u0435\u043e\u0440\u0433\u0438\u0439 \u043d\u0430\u0447\u0430\u043b \u0440\u0430\u0431\u043e\u0442\u0443 \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u043e\u043c \u0432 \u0433\u0430\u0437\u0435\u0442\u0435 \u00ab\u041a\u0430\u043d\u0430\u0448\u00bb \u0438 \u0442\u0440\u0435\u0441\u0442\u0435 \u00ab\u0427\u0443\u0432\u0430\u0448\u043a\u0438\u043d\u043e\u00bb, \u0430 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440 \u0441\u0442\u0430\u043b \u043a\u043e\u0440\u0440\u0435\u0441\u043f\u043e\u043d\u0434\u0435\u043d\u0442\u043e\u043c \u0433\u0430\u0437\u0435\u0442\u044b \u00ab\u041a\u0440\u0430\u0441\u043d\u0430\u044f \u0427\u0443\u0432\u0430\u0448\u0438\u044f\u00bb. \u0412\u043c\u0435\u0441\u0442\u0435 \u0441 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u043c \u043e\u043d\u0438 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u043b\u0438 \u0443\u0447\u0430\u0441\u0442\u0438\u0435 \u0432 \u0432\u044b\u0435\u0437\u0434\u043d\u044b\u0445 \u0431\u0440\u0438\u0433\u0430\u0434\u0430\u0445, \u0435\u0437\u0434\u0438\u0432\u0448\u0438\u0445 \u043f\u043e \u0440\u0430\u0439\u043e\u043d\u0430\u043c \u0440\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u0438 \u0432\u044b\u043f\u0443\u0441\u043a\u0430\u044e\u0449\u0438\u0445 \u0444\u043e\u0442\u043e\u0433\u0430\u0437\u0435\u0442\u044b.\n\n\u0421\u043e\u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u044b \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0430: \u0423\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0435 \u043c\u043e\u043b\u043e\u0434\u0435\u0436\u043d\u043e\u0439 \u043f\u043e\u043b\u0438\u0442\u0438\u043a\u0438 \u041c\u0438\u043d\u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u0427\u0443\u0432\u0430\u0448\u0438\u0438, \u0413\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0438\u0441\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0430\u0440\u0445\u0438\u0432 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438, \u041e\u0431\u044a\u0435\u0434\u0438\u043d\u0435\u043d\u0438\u0435 \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u043a \u0433. \u0427\u0435\u0431\u043e\u043a\u0441\u0430\u0440\u044b.\n\n\u0421 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435\u043c \u043e \u0444\u043e\u0442\u043e\u043a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u043c\u043e\u0436\u043d\u043e \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u0441\u0430\u0439\u0442\u0435 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f \u0438 \u0432 \u0435\u0433\u043e \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u0435 \u0412\u041a\u043e\u043d\u0442\u0430\u043a\u0442\u0435.\n\n@alena_arshinova\n#\u041a\u0440\u0430\u0441\u043a\u0438_\u0427\u0443\u0432\u0430\u0448\u0438\u0438 #kraski_konkurs","link":"https:\/\/www.instagram.com\/p\/B-WmMZ8DvU2","type":"sidecar","fullsize":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/p1080x1080\/91285421_3067445923481765_5490263871638981073_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=105&_nc_ohc=59r3JfBnixIAX-OhkFv&oh=7db911506e6c14780ec9601dc092640a&oe=5EAD7EDC","large":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/sh0.08\/e35\/c0.5.1219.1219a\/s640x640\/91285421_3067445923481765_5490263871638981073_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=105&_nc_ohc=59r3JfBnixIAX-OhkFv&oh=7fe6091c9ef582fbe186ab249cbca16c&oe=5EAD7A45","small":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/c0.5.1219.1219a\/s320x320\/91285421_3067445923481765_5490263871638981073_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=105&_nc_ohc=59r3JfBnixIAX-OhkFv&oh=c6f970491a651eab11d0c41ff45e5e1e&oe=5EAD187C","likesCount":11,"commentsCount":0,"authorId":9227120312},{"id":"2276124438737831318","code":"B-Wax4bjcWW","created":1585555198,"text":"[\u041a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0430\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430]\n\n\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0434\u0435\u0442\u0441\u043a\u043e\u0433\u043e \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u0430 \u0438\u043c\u0435\u043d\u0438 \u0410. \u041a\u043e\u043a\u0435\u043b\u044f\n\n\u041a \u0443\u0447\u0430\u0441\u0442\u0438\u044e \u0432 \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0435 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u044e\u0442\u0441\u044f \u0434\u0435\u0442\u0441\u043a\u0438\u0435 \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435 \u0440\u0430\u0431\u043e\u0442\u044b (\u0436\u0438\u0432\u043e\u043f\u0438\u0441\u044c, \u0433\u0440\u0430\u0444\u0438\u043a\u0430, \u0434\u0435\u043a\u043e\u0440\u0430\u0442\u0438\u0432\u043d\u043e-\u043f\u0440\u0438\u043a\u043b\u0430\u0434\u043d\u043e\u0435 \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u043e, \u043a\u043e\u043b\u043b\u0430\u0436), \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d\u043d\u044b\u0435 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u0435, \u0438\u0441\u0442\u043e\u0440\u0438\u0438, \u043f\u0440\u0438\u0440\u043e\u0434\u043d\u043e\u043c\u0443 \u0434\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044e \u0427\u0443\u0432\u0430\u0448\u0438\u0438\n\n\u0414\u043b\u044f \u0443\u0447\u0430\u0441\u0442\u0438\u044f \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e: \n\u043f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0443, \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u0435\u0451 \u043d\u0430 \u0441\u0430\u0439\u0442 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f, \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0432 \u0430\u043d\u043a\u0435\u0442\u0443\n\n\u0421 2019 \u0433\u043e\u0434\u0430 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043d\u043e\u0441\u0438\u0442 \u0438\u043c\u044f \u0445\u0443\u0434\u043e\u0436\u043d\u0438\u043a\u0430 \u0410.\u0410. \u041a\u043e\u043a\u0435\u043b\u044f\n\n\u0410\u043b\u0435\u043a\u0441\u0435\u0439 \u0410\u0444\u0430\u043d\u0430\u0441\u044c\u0435\u0432\u0438\u0447 \u041a\u043e\u043a\u0435\u043b\u044c - \u0436\u0438\u0432\u043e\u043f\u0438\u0441\u0435\u0446, \u0433\u0440\u0430\u0444\u0438\u043a, \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440, \u043f\u0435\u0440\u0432\u044b\u0439 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u0445\u0443\u0434\u043e\u0436\u043d\u0438\u043a \u0441 \u0430\u043a\u0430\u0434\u0435\u043c\u0438\u0447\u0435\u0441\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c. \u041e\u0441\u043d\u043e\u0432\u043e\u043f\u043e\u043b\u043e\u0436\u043d\u0438\u043a \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0433\u043e \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0438\u0437\u043e\u0431\u0440\u0430\u0437\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u0430 \u0438 \u043e\u0434\u0438\u043d \u0438\u0437 \u0441\u043e\u0437\u0434\u0430\u0442\u0435\u043b\u0435\u0439 \u0441\u0442\u0430\u043d\u043a\u043e\u0432\u043e\u0439 \u0436\u0438\u0432\u043e\u043f\u0438\u0441\u0438 \u0423\u043a\u0440\u0430\u0438\u043d\u044b. \u0412 1903 \u0433. \u043e\u043a\u043e\u043d\u0447\u0438\u043b \u041d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u0443\u044e \u0440\u0438\u0441\u043e\u0432\u0430\u043b\u044c\u043d\u0443\u044e \u0448\u043a\u043e\u043b\u0443 \u043f\u0440\u0438 \u0410\u043a\u0430\u0434\u0435\u043c\u0438\u0438 \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432, \u0430 \u0432 1912 \u0433. \u2013 \u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0441\u043a\u0443\u044e \u0410\u043a\u0430\u0434\u0435\u043c\u0438\u044e \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432 \u043f\u043e \u043c\u0430\u0441\u0442\u0435\u0440\u0441\u043a\u043e\u0439 \u0418. \u0415. \u0420\u0435\u043f\u0438\u043d\u0430 \u0438 \u0414. \u041d. \u041a\u0430\u0440\u0434\u043e\u0432\u0441\u043a\u043e\u0433\u043e. \u201c...\u041a\u043e\u043a\u0435\u043b\u044c \u2013 \u0445\u0443\u0434\u043e\u0436\u043d\u0438\u043a \u043d\u0435\u043e\u0431\u044b\u0447\u043d\u044b\u0439. \u0415\u0433\u043e \u0432\u044b\u0441\u043e\u043a\u043e \u0446\u0435\u043d\u0438\u043b \u0418. \u0415. \u0420\u0435\u043f\u0438\u043d. \u042f\u0432\u043b\u044f\u043b\u0441\u044f \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u043e\u043c \u0438 \u043f\u0435\u0440\u0432\u044b\u043c \u0440\u0435\u043a\u0442\u043e\u0440\u043e\u043c \u0425\u0430\u0440\u044c\u043a\u043e\u0432\u0441\u043a\u043e\u0439 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u0430\u043a\u0430\u0434\u0435\u043c\u0438\u0438 \u0434\u0438\u0437\u0430\u0439\u043d\u0430 \u0438 \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432, \u0432 \u043a\u043e\u0442\u043e\u0440\u043e\u043c \u0434\u043e \u043a\u043e\u043d\u0446\u0430 \u0436\u0438\u0437\u043d\u0438 \u043f\u0440\u043e\u0440\u0430\u0431\u043e\u0442\u0430\u043b \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440\u043e\u043c \u0438 \u0437\u0430\u0432\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u043a\u0430\u0444\u0435\u0434\u0440\u043e\u0439 \u0440\u0438\u0441\u0443\u043d\u043a\u0430. \u0420\u0430\u0431\u043e\u0442\u0430\u043b \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440\u043e\u043c \u041a\u0438\u0435\u0432\u0441\u043a\u043e\u0433\u043e \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0430. \u041e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440 \u0438 \u043f\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043b\u044c \u0410\u0441\u0441\u043e\u0446\u0438\u0430\u0446\u0438\u0438 \u0445\u0443\u0434\u043e\u0436\u043d\u0438\u043a\u043e\u0432 \u0427\u0435\u0440\u0432\u043e\u043d\u043d\u043e\u0439 \u0423\u043a\u0440\u0430\u0438\u043d\u044b. \u0423\u0447\u0430\u0441\u0442\u043d\u0438\u043a \u043c\u0435\u0436\u0434\u0443\u043d\u0430\u0440\u043e\u0434\u043d\u044b\u0445, \u0432\u0441\u0435\u0441\u043e\u044e\u0437\u043d\u044b\u0445, \u0440\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0430\u043d\u0441\u043a\u0438\u0445 \u0438 \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0445 \u0432\u044b\u0441\u0442\u0430\u0432\u043e\u043a. \u041e\u0441\u043d\u043e\u0432\u043d\u044b\u0435 \u0440\u0430\u0431\u043e\u0442\u044b: \u201c\u041f\u043e\u0440\u0442\u0440\u0435\u0442 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0436\u0435\u043d\u0449\u0438\u043d\u044b\u201d, \u201c\u0423\u0440\u0430\u043b\u044c\u0441\u043a\u0438\u0435 \u0437\u0430\u0432\u043e\u0434\u044b\u201d, \u201c\u0418\u0442\u0430\u043b\u044c\u044f\u043d\u0446\u044b\u201d, \u201c\u0418\u0437 \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0441\u044e\u0438\u0442\u044b\u201d, \u201c\u0418\u0437\u0432\u043e\u0437\u0447\u0438\u043a\u201d, \u201c\u0412\u0435\u043d\u0447\u0430\u043d\u0438\u0435. \u0427\u0443\u0432\u0430\u0448\u0438\u201d, \u201c\u0412 \u0447\u0430\u0439\u043d\u043e\u0439\u201d, \u201c\u0421\u0435\u043c\u0435\u0439\u043d\u044b\u0439 \u043f\u043e\u0440\u0442\u0440\u0435\u0442\u201d, \u201c\u0421\u0442\u0430\u0440\u044b\u0439 \u0440\u0430\u0431\u043e\u0447\u0438\u0439\u201d, \u201c\u041d\u0430 \u043f\u043e\u0441\u0442\u0443\u201d, \u201c\u041d\u0430\u0433\u0440\u0430\u0436\u0434\u0435\u043d\u0438\u0435 \u0443\u0434\u0430\u0440\u043d\u0438\u0446\u044b\u201d, \u201c\u041b\u0438\u043a\u0431\u0435\u0437\u201d,, \u201c\u0417\u0438\u043c\u0430\u201d \u0438 \u0434\u0440. \u041d\u0430 \u043c\u043e\u0433\u0438\u043b\u0435 \u0410. \u041a\u043e\u043a\u0435\u043b\u044f \u0432 \u0425\u0430\u0440\u044c\u043a\u043e\u0432\u0435 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d \u043f\u0430\u043c\u044f\u0442\u043d\u0438\u043a. \u0418\u043c\u0435\u043d\u0435\u043c \u0445\u0443\u0434\u043e\u0436\u043d\u0438\u043a\u0430 \u043d\u0430\u0437\u0432\u0430\u043d\u0430 \u0443\u043b\u0438\u0446\u0430 \u0432 \u0433. \u0427\u0435\u0431\u043e\u043a\u0441\u0430\u0440\u044b. \u0412 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u043c \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u043c \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0435 \u0438\u043c. \u0418.\u041d. \u0423\u043b\u044c\u044f\u043d\u043e\u0432\u0430 \u0432 2009 \u0433. \u043e\u0442\u043a\u0440\u044b\u0442 \u0425\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043c\u0443\u0437\u0435\u0439 \u0438\u043c. \u0410. \u0410. \u041a\u043e\u043a\u0435\u043b\u044f\n\n\u0421 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435\u043c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u0430 \u0412\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u0441\u0430\u0439\u0442\u0435 \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u044f \u0438 \u0432 \u0435\u0433\u043e \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u0435 \u0412\u041a\u043e\u043d\u0442\u0430\u043a\u0442\u0435.\n\n\u0421\u043e\u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0442\u043e\u0440\u044b \u041a\u043e\u043d\u043a\u0443\u0440\u0441\u0430 \u0427\u0413\u041f\u0423 \u0438\u043c. \u0418.\u042f. \u042f\u043a\u043e\u0432\u043b\u0435\u0432\u0430, \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u0438\u0439 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u043c\u0443\u0437\u0435\u0439.\n\n@alena_arshinova\n#\u041a\u0440\u0430\u0441\u043a\u0438_\u0427\u0443\u0432\u0430\u0448\u0438\u0438 #kraski_konkurs","link":"https:\/\/www.instagram.com\/p\/B-Wax4bjcWW","type":"sidecar","fullsize":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/p1080x1080\/91147484_898850040570686_4985024415546285588_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=102&_nc_ohc=xIYtPEmgpcoAX82cCh6&oh=dba6b6aa717f49751fc6258aa841f28f&oe=5EADF939","large":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/sh0.08\/e35\/c0.5.1219.1219a\/s640x640\/91147484_898850040570686_4985024415546285588_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=102&_nc_ohc=xIYtPEmgpcoAX82cCh6&oh=3a4c265cc27500ce5a6ab42c7fe37f29&oe=5EAE132E","small":"https:\/\/instagram.fhen2-1.fna.fbcdn.net\/v\/t51.2885-15\/e35\/c0.5.1219.1219a\/s320x320\/91147484_898850040570686_4985024415546285588_n.jpg?_nc_ht=instagram.fhen2-1.fna.fbcdn.net&_nc_cat=102&_nc_ohc=xIYtPEmgpcoAX82cCh6&oh=be1d936e40eeaeac42b2c38bb25e71a6&oe=5EAD2D36","likesCount":10,"commentsCount":0,"authorId":9227120312},{"id":"2275621920350090808","code":"B-UohRtjzY4","created":1585495294,"text":"\u041a\u0440\u0430\u0441\u043a\u0438 \u0427\u0443\u0432\u0430\u0448\u0438\u0438-2020: \u043e\u0442\u043a\u0440\u044b\u0442 \u043f\u0440\u0438\u0435\u043c \u0440\u0430\u0431\u043e\u0442!\n\n\u041a 100-\u043b\u0435\u0442\u0438\u044e \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0430\u0432\u0442\u043e\u043d\u043e\u043c\u0438\u0438, \u043f\u043e\u043b\u044e\u0431\u0438\u0432\u0448\u0438\u0439\u0441\u044f \u043c\u043d\u043e\u0433\u0438\u043c \u0436\u0438\u0442\u0435\u043b\u044f\u043c \u0438 \u0434\u0440\u0443\u0437\u044c\u044f\u043c \u0440\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u043a\u043e\u043d\u043a\u0443\u0440\u0441 #\u041a\u0440\u0430\u0441\u043a\u0438\u0427\u0443\u0432\u0430\u0448\u0438\u0438 \u0441\u0442\u0430\u043b \u043c\u0430\u0441\u0448\u0442\u0430\u0431\u043d\u044b\u043c \u0444\u0435\u0441\u0442\u0438\u0432\u0430\u043b\u0435\u043c.\n\n\u041a \u0443\u0436\u0435 \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u043e\u043d\u043d\u044b\u043c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u044b\u043c \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f\u043c (\u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0435 \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u043e, \u0444\u043e\u0442\u043e\u043c\u0430\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u043e, \u0432\u043e\u043a\u0430\u043b\u044c\u043d\u043e-\u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u043e\u0435 \u0438 \u0442\u0430\u043d\u0446\u0435\u0432\u0430\u043b\u044c\u043d\u043e\u0435 \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u043e) \u0434\u043e\u0431\u0430\u0432\u0438\u043b\u0441\u044f \u043b\u0438\u0442\u0435\u0440\u0430\u0442\u0443\u0440\u043d\u044b\u0439 \u043a\u043e\u043d\u043a\u0443\u0440\u0441, \u0430 \u0442\u0430\u043a\u0436\u0435 \u043a\u043e\u043d\u043a\u0443\u0440\u0441 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0439 \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u043a\u0440\u0430\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u0438 \u0433\u0435\u043d\u0435\u0430\u043b\u043e\u0433\u0438\u0438.\n\n\u041a\u043e\u043d\u043a\u0443\u0440\u0441\u044b \u043f\u043e\u043b\u0443\u0447\u0438\u043b\u0438 \u0438\u043c\u0435\u043d\u0430 \u0432\u044b\u0434\u0430\u044e\u0449\u0438\u0445\u0441\u044f \u043b\u044e\u0434\u0435\u0439 \u0427\u0443\u0432\u0430\u0448\u0438\u0438:\n\ud83c\udfa8 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0434\u0435\u0442\u0441\u043a\u043e\u0433\u043e \u0445\u0443\u0434\u043e\u0436\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u0430 \u2014 \u0438\u043c\u044f \u043f\u0435\u0440\u0432\u043e\u0433\u043e \n\u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0433\u043e \u0445\u0443\u0434\u043e\u0436\u043d\u0438\u043a\u0430 \u0441 \u0430\u043a\u0430\u0434\u0435\u043c\u0438\u0447\u0435\u0441\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u0410. \u041a\u043e\u043a\u0435\u043b\u044f\n\ud83d\udcf8 \u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0444\u043e\u0442\u043e\u043c\u0430\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0430 \u2014 \u0438\u043c\u044f \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u043e\u0432, \u0437\u0430\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u0432\u0448\u0438\u0435 \u043f\u0435\u0440\u0432\u044b\u0435 \u0448\u0430\u0433\u0438 \u0427\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438, \u0431\u0440\u0430\u0442\u044c\u0435\u0432 \u041a\u043e\u0441\u0442\u0438\u043d\u044b\u0445\n\ud83c\udfa4\ud83c\udfbb\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0432\u043e\u043a\u0430\u043b\u044c\u043d\u043e-\u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u0430 \u2014 \u0438\u043c\u044f \u0432\u044b\u0434\u0430\u044e\u0449\u0435\u0433\u043e\u0441\u044f \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0433\u043e \u043f\u0435\u0432\u0446\u0430, \u043b\u0430\u0443\u0440\u0435\u0430\u0442\u0430 \u0413\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u043f\u0440\u0435\u043c\u0438\u0438 \u0421\u0421\u0421\u0420, \u043d\u0430\u0440\u043e\u0434\u043d\u043e\u0433\u043e \u0430\u0440\u0442\u0438\u0441\u0442\u0430 \u0421\u0421\u0421\u0420 \u041c\u0430\u043a\u0441\u0438\u043c\u0430 \u041c\u0438\u0445\u0430\u0439\u043b\u043e\u0432\u0430\n\ud83d\udc83\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0442\u0430\u043d\u0446\u0435\u0432\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u0430 \u2014 \u0438\u043c\u044f \u0431\u0430\u043b\u0435\u0440\u0438\u043d\u044b, \u043d\u0430\u0440\u043e\u0434\u043d\u043e\u0439 \u0430\u0440\u0442\u0438\u0441\u0442\u043a\u0438 \u0421\u0421\u0421\u0420 \u0438 \u0420\u0421\u0424\u0421\u0420 \u041d\u0430\u0434\u0435\u0436\u0434\u044b \u041f\u0430\u0432\u043b\u043e\u0432\u043e\u0439\n\ud83d\udcdd\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043c\u0430\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0430 \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u043e\u0432 \u0443\u0447\u0440\u0435\u0436\u0434\u0435\u043d\u0438\u0439 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b \u0438 \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u2014 \u0438\u043c\u044f \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0433\u043e \u0443\u0447\u0435\u043d\u043e\u0433\u043e, \u043e\u0441\u043d\u043e\u0432\u0430\u0442\u0435\u043b\u044f \u044d\u0442\u043d\u043e\u043f\u0435\u0434\u0430\u0433\u043e\u0433\u0438\u043a\u0438 \u0413\u0435\u043d\u043d\u0430\u0434\u0438\u044f \u0412\u043e\u043b\u043a\u043e\u0432\u0430\n\ud83d\udd0d\ud83d\udcdc\u041a\u043e\u043d\u043a\u0443\u0440\u0441 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0439 \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u043a\u0440\u0430\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u0438 \u0433\u0435\u043d\u0435\u0430\u043b\u043e\u0433\u0438\u0438 \u2014 \u0438\u043c\u044f \u0434\u043e\u043a\u0442\u043e\u0440\u0430 \u0438\u0441\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043d\u0430\u0443\u043a, \u044d\u0442\u043d\u043e\u0433\u0440\u0430\u0444\u0430, \u043f\u0435\u0440\u0432\u043e\u0433\u043e \u0447\u0443\u0432\u0430\u0448\u0441\u043a\u043e\u0433\u043e \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440\u0430 \u041d\u0438\u043a\